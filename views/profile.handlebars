<head>
  <meta charset="utf-8">
  <title>Welcome to your profile!</title>
</head>
<body>
  <h1>Search Recipes:</h1>
  
  <form id="search-form">
    <label for="search-input">Search:</label>
    <input type="text" id="search-input" name="search" placeholder="Enter keywords">
    <div class="progress">
      <div class="indeterminate"></div>
  </div>
     <button id="search-button" class="waves-effect waves-light btn"><i class="material-icons right">search</i>Search</button>
    {{!-- <button type="submit">Search</button> --}}
  </form>
  
  <ul>
    {{#each recipes}}
      <li>
        <h1>{{this.Title}}</h1>
        <h2>"{{this.Ingredient}}</h2>
        <p>{{this.Instructions}}</p>
        <a href="../public/images/Food Images/{{this.Image_Name}}">View Recipe</a>
      </li>
    {{/each}}
  </ul>
  <button class="waves-effect waves-light btn"><i class="material-icons right">save</i>Save Recipe</button>
  
  
  <script src="/js/profile.js"></script>
</body>